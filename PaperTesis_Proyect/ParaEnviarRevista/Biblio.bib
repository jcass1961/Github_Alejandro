@Article{MellinAnalysisPolSAR,
  author   = {Anfinsen, S. N. and Eltoft, T.},
  title    = {Application of the Matrix-Variate {M}ellin Transform to Analysis of Polarimetric Radar Images},
  doi      = {10.1109/TGRS.2010.2103945},
  issn     = {0196-2892},
  number   = {6},
  pages    = {2281--2295},
  volume   = {49},
  abstract = {In this paper, we propose to use a matrix-variate Mellin transform
	in the statistical analysis of multilook polarimetric radar data.
	The domain of the transform integral is the cone of complex positive
	definite matrices, which allows for transformation of the distributions
	used to model the polarimetric covariance and coherency matrix. Based
	on the matrix-variate Mellin transform, an alternative characteristic
	function is defined, from which we can retrieve a new kind of matrix
	log-moments and log-cumulants. It is demonstrated that the matrix
	log-cumulants are of great value to analysis of polarimetric radar
	data, and that they can be used to derive estimators for the distribution
	parameters with low bias and variance.},
  journal  = {IEEE Transactions on Geoscience and Remote Sensing},
  keywords = {coherency matrix;complex positive definite matrices;distribution parameters;doubly stochastic product model;matrix log-cumulants;matrix log-moments;matrix-variate Mellin transform;matrix-variate statistics;multilook polarimetric radar data;polarimetric covariance;polarimetric radar images;statistical analysis;synthetic aperture radar;transform integral;geophysical image processing;geophysical techniques;radar polarimetry;statistical analysis;stochastic processes;synthetic aperture radar;},
  year     = {2011},
}

