@Article{PolarimetricSegmentationBSplinesMSSP,
  author     = {A. C. Frery and Jacobo-Berlles, J. and Gambini, J. and Mejail, M.},
  title      = {Polarimetric {SAR} Image Segmentation with {B-Splines} and a New Statistical Model},
